config {
  type: "operations",
  dependencies: ["merge_alerts"]
}

-- Export data from the target table to GCS
EXPORT DATA OPTIONS (
  uri = 'gs://bq-alerting/bq-alerting-*.csv',
  format = 'CSV',
  overwrite = true
) AS

SELECT *
FROM `${dataform.projectConfig.vars.project}.${dataform.projectConfig.vars.dataset}`.bq_alerting
WHERE incident_processed is False